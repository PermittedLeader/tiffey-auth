{
    "name": "permittedleader/tiffey-auth",
    "description": "Role and Permission management for Laravel using the Tiffey layout engine",
    "type": "library",
    "authors": [
        {
            "name": "Myles Smith",
            "email": "159888845+PermittedLeader@users.noreply.github.com"
        }
    ],
    "require": {
        "laravel/framework": "^10.45",
        "permittedleader/tiffey": "^1.4.4",
        "permittedleader/tiffey-tables": "^4.1.0",
        "permittedleader/flash-messages": "^1.0",
        "livewire/livewire": "^3.5",
        "spatie/laravel-permission": "^6.9"
    },
    "require-dev": {
        "orchestra/testbench": "^8.21",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Permittedleader\\TiffeyAuth\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Permittedleader\\TiffeyAuth\\TiffeyAuthServiceProvider"
            ]
        }
    }
}
